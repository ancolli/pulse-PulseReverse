FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    axis
    {
	type            empty;     
    }
    
    "wedge.*"
    {
        type            wedge;
    } 
    
    "wall_isolation.*"
    {
        //type            zeroGradient;//symmetry;
        //type            totalPressure;
        //p0              uniform 0;
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
    wall_WE
    {
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
    wall_CE
    {
        //type            zeroGradient;//symmetry;
        type            fixedFluxPressure;
        rho             rhok;
        value           uniform 0;
    }
}


